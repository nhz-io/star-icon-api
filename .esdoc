{
  "source": "./src",
  "destination": "./doc",
  "includes": ["\\.js$"],
  "unexportIdentifier": false,
  "undocumentIdentifier": true,
  "builtinExternal": true,
  "index": "./readme.md",
  "package": "./package.json",
  "coverage": true,
  "includeSource": true,
  "lint": true,
  "plugins": [
    {"name": "esdoc-es7-plugin"}
  ]
}
